<mujoco model="insole">
	<compiler strippath="true" meshdir="meshes/" />
	<extension>
		<plugin plugin="mujoco.elasticity.membrane" />
	</extension>
	<worldbody>
		<body name="insole">
			<flexcomp name="insole" pos=".12 -.05 .452" type="mesh" file="insole.stl" rgba=".1 .9 .1 1">
				<edge equality="true" />
				<plugin plugin="mujoco.elasticity.membrane">
					<config key="young" value="200000.0" />
					<config key="poisson" value="0.2" />
					<config key="thickness" value=".01" />
				</plugin>
			</flexcomp>
		</body>
	</worldbody>
</mujoco>
